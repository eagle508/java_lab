class calculator {


    int add(int a, int b) {
        return a + b;
    }


    int add(int a, int b, int c) {
        return a + b + c;
    }


    double add(double a, double b) {
        return a + b;
    }
}

public class Experiment5 {
    public static void main(String[] args) {
        calculator exp = new calculator();

        System.out.println(exp.add(10, 20));
        System.out.println(exp.add(10, 20, 30));
        System.out.println( exp.add(5.5, 4.5));
    }
}
