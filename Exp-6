class Experiment {

    // Method with 2 integer parameters
    int add(int a, int b) {
        return a + b;
    }

    // Method with 3 integer parameters (same name, different parameters)
    int add(int a, int b, int c) {
        return a + b + c;
    }

    // Method with 2 double parameters (same name, different type)
    double add(double a, double b) {
        return a + b;
    }
}

public class MethodOverloading {
    public static void main(String[] args) {
        Experiment exp = new Experiment();

        System.out.println(exp.add(10, 20));
        System.out.println(exp.add(10, 20, 30));
        System.out.println( exp.add(5.5, 4.5));
    }
}
